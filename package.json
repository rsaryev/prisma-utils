{
  "name": "prisma-utils",
  "version": "1.0.1",
  "description": "utils for prisma",
  "main": "dist/index.js",
  "author": "rsaryev1997@gmail.com",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/rsaryev/prisma-utils.git"
  },
  "issues": {
    "url": "https://github.com/rsaryev/prisma-utils/issues"
  },
  "scripts": {
    "test": "jest --watchAll --setupFiles dotenv/config",
    "build": "tsc"
  },
  "jest": {
    "preset": "ts-jest",
