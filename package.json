    "testEnvironment": "node"
  },
  "keywords": [
    "prisma",
    "utils",
    "typescript",
    "clear",
    "delete",
    "test"
  ],
  "files": [
    "dist",
    "./readme.md"
  ],
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.4.3",
    "postgres": "^3.3.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
